@model Product



<div class="col-md-4 my-3">

<div class="card" style="width: 18rem;">
  <img src="/images/default.jpg" class="card-img-top" alt="...">
  <div class="card-body">
    <h5 class="card-title">@Model.ProductName</h5>
    <p class="card-text">@Model.ProductPrice </p>
    <a  asp-action="Get" asp-route-id=@Model.ProdcutId class="btn btn-primary">
        <i class="fa-solid fa-arrow-right"></i> Go </a>
  </div>
</div>
</div>


